0430\u044e\u0442\u0441\u044f \u0432 \u0443\u0435\u0437\u0434\u043d\u044b\u0445\n\u0433\u043e\u0440\u043e\u0434\u0430\u0445. \u041f\u043e \u043b\u0435\u0432\u0443\u044e \u0440\u0443\u043a\u0443 <\/em><em>\u0437\u0430 \u0432\u043e\u043b\u043d\u0438\u0441\u0442\u044b\u043c\u0438 <\/em><em>\u0437\u0435\u043b\u0435\u043d\u043e\u0432\u0430\u0442\u044b\u043c\u0438\n\u0441\u0442\u0435\u043a\u043b\u0430\u043c\u0438 \u0441\u0435\u0440\u0435\u0431\u0440\u0438\u043b\u0438\u0441\u044c <\/em><em>\u0433\u0440\u043e\u0431\u044b <\/em><em>\u043f\u043e\u0445\u043e\u0440\u043e\u043d\u043d\u043e\u0433\u043e \u0431\u044e\u0440\u043e <\/em><em>\u00ab\u041d\u0438\u043c\u0444\u0430\u00bb. \u0421\u043f\u0440\u0430\u0432\u0430 \u0437\u0430 <\/em><em>\u043c\u0430\u043b\u0435\u043d\u044c\u043a\u0438\u043c\u0438,\n\u0441 \u043e\u0431\u0432\u0430\u043b\u0438\u0432\u0448\u0435\u0439\u0441\u044f <\/em><em>\u0437\u0430\u043c\u0430\u0437\u043a\u043e\u0439 <\/em><em>\u043e\u043a\u043d\u0430\u043c\u0438 \u0443\u0433\u0440\u044e\u043c\u043e \u0432\u043e\u0437\u043b\u0435\u0436\u0430\u043b\u0438 \u0434\u0443\u0431\u043e\u0432\u044b\u0435 <\/em><em>\u043f\u044b\u043b\u044c\u043d\u044b\u0435 <\/em><em>\u0433\u0440\u043e\u0431\u044b\n\u0433\u0440\u043e\u0431\u043e\u00ad\u0432\u044b\u0445 \u0434\u0435\u043b \u043c\u0430\u0441\u0442\u0435\u0440\u0430 \u0411\u0435\u0437\u0435\u043d\u0447\u0443\u043a\u0430.<\/em><em><\/em><\/p>\n\n\n\n<p><em>&#8212; \u00ab\u041d\u0438\u043c\u0444\u0430\u00bb, \u0442\u0443\u0434\u044b \u0435\u0435 <\/em><em>\u0432\n\u043a\u0430\u0447\u0435\u043b\u044c, \u0440\u0430\u0437\u0432\u0435 \u0442\u043e\u0432\u0430\u0440 \u0434\u0430\u0435\u0442? &#8212; \u0441\u043c\u0443\u0442\u043d\u043e <\/em><em>\u043c\u043e\u043b\u0432\u0438\u043b <\/em><em>\u0433\u0440\u043e\u0431\u043e\u0432\u043e\u0439\n\u043c\u0430\u0441\u0442\u0435\u0440 \u0411\u0435\u0437\u0435\u043d\u0447\u0443\u043a. \u0420\u0430\u0437\u0432\u0435 <\/em><em>\u0436 <\/em><em>\u043e\u043d\u043e <\/em><em>\u043c\u043e\u0436\u0435\u0442 <\/em><em>\u043f\u043e\u043a\u0443\u043f\u0430\u0442\u0435\u043b\u044f\n\u0443\u0434\u043e\u0432\u043b\u0435\u0442\u0432\u043e\u0440\u0438\u0442\u044c? <\/em><em>\u0413\u0440\u043e\u0431 <\/em><em>&#8212; \u043e\u043d \u043e\u0434\u043d\u043e\u0433\u043e \u043b\u0435\u0441\u0443 \u0441\u043a\u043e\u043b\u044c\u043a\u043e \u0442\u0440\u0435\u0431\u0443\u0435\u0442&#8230; \u0414\u0430 <\/em><em>\u0432\u043e\u0442. \u041d\u0438\u043c\u0444\u0430\u00bb&#8230; <\/em><em>\u0423\u0436\u0435\n\u0443 \u043d\u0438\u0445 <\/em><em>\u0438 <\/em><em>\u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b\n\u043d\u0435 \u0442\u043e\u0442, \u0438 \u043e\u0442\u0434\u0435\u043b\u043a\u0435 <\/em><em>\u043f\u043e\u0445\u0443\u0436\u0435, <\/em><em>\u0438 \u043a\u0438\u0441\u0442\u044c \u0436\u0438\u0434\u043a\u0430\u044f, \u0442\u0443\u0434\u044b \u0435\u0435 \u0432 \u043a\u0430\u0447\u0435\u043b\u044c.\n\u0410 \u044f &#8212; <\/em><em>\u0444\u0438\u0440\u043c\u0430\n<\/em><em>\u0441\u0442\u0430\u0440\u0430\u044f.\n\u0423 <\/em><em>\u043c\u0435\u043d\u044f\n<\/em><em>\u0433\u0440\u043e\u0431\n&#8212; \u043e\u0433\u0443\u0440\u0447\u0438\u043a, \u043e\u0442\u0431\u043e\u0440\u043d\u044b\u0439, \u043b\u044e\u0431\u0438\u0442\u0435\u043b\u044c\u0441\u043a\u0438\u0439&#8230;\u00bb<\/em><em><\/em><\/p>\n\n\n\n<p><em>\u0418.\u0418\u043b\u044c\u0444, <\/em><em>\u0415.\u041f\u0435\u0442\u0440\u043e\u0432\n&#171;\u0414\u0432\u0435\u043d\u0430\u0434\u0446\u0430\u0442\u044c \u0441\u0442\u0443\u043b\u044c\u0435\u0432&#187;.<\/em><\/p>\n\n\n\n<p>\u0421\u0430\u0440\u0430\u0442\u043e\u0432 \u043c\u0430\u043b\u043e \u0447\u0435\u043c \u043e\u0442\u043b\u0438\u0447\u0430\u0435\u0442\u0441\u044f \u043e\u0442 \u0433\u043e\u0440\u043e\u0434\u0430 N. \u0417\u0434\u0435\u0441\u044c \u0443\u0436\u0435\n\u043e\u0434\u0438\u043d\u043d\u0430\u0434\u0446\u0430\u0442\u044c \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u043d\u044b\u0445 \u0441\u043b\u0443\u0436\u0431, \u0438 \u044d\u0442\u043e, \u043a\u0430\u043a \u0433\u043e\u0432\u043e\u0440\u0438\u0442\u0441\u044f, \u0435\u0449\u0435 \u043d\u0435 \u043f\u0440\u0435\u0434\u0435\u043b. \u0412\u043e\u0442 \u0438\n\u00ab\u0431\u0440\u043e\u0434\u044f\u0442 \u043f\u043e \u0433\u043e\u00ad\u0440\u043e\u0434\u0443\u00bb \u0411\u0435\u0437\u0435\u043d\u0447\u0443\u043a\u0438, \u0437\u0430\u043c\u0430\u043d\u0438\u0432\u0430\u044f \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u043e\u0431\u0435\u0449\u0430\u043d\u0438\u044f\u00ad\u043c\u0438 \u0432\u0441\u0435\u0432\u043e\u0437\u043c\u043e\u0436\u043d\u044b\u0445\n\u0431\u043b\u0430\u0433. \u041a\u0430\u0436\u0434\u044b\u0439 \u043f\u043e\u0442\u0435\u043d\u0446\u0438\u0430\u043b\u044c\u043d\u043e \u0443\u043c\u0438\u00ad\u0440\u0430\u044e\u0449\u0438\u0439 \u043d\u0430 \u0443\u0447\u0435\u0442\u0435, \u041a\u0430\u0436\u0434\u044b\u0439 \u043c\u043e\u0440\u0433 \u00ab\u0441\u0445\u0432\u0430\u0447\u0435\u043d\u00bb&#8230; \u0418 \u0435\u0449\u0435\n\u0433\u0440\u0430\u0431\u043e\u0432\u044b\u0445 \u0434\u0435\u043b \u043c\u0430\u0441\u0442\u0435\u0440\u0430 \u043f\u0440\u043e \u043a\u043e\u043d\u043a\u0443\u0440\u0435\u043d\u0442\u043e\u0432 \u043c\u0430\u0441\u0442\u0430\u043a\u0438 \u0440\u0430\u0441\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0442\u044c: \u043c\u043e\u043b, \u00ab\u043c\u0430\u0442\u0435\u0440\u0438\u0430\u043b \u043d\u0435\n\u0442\u043e\u0442, \u0438 \u043e\u0442\u0434\u0435\u043b\u043a\u0430 \u043f\u043e\u0445\u0443\u0436\u0435\u00bb. \u0410 \u0445\u0443\u0434\u0430 \u0434\u0435\u0432\u0430\u0442\u044c\u0441\u044f? \u0415\u0441\u043b\u0438 \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u043d\u044b\u0445 \u0441\u043b\u0443\u0436\u0431 \u043d\u0430 \u0434\u0443\u0448\u0443\n\u043d\u0430\u0441\u0435\u043b\u0435\u043d\u0438\u044f \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f \u0441 \u0438\u0437\u0431\u044b\u0442\u043a\u043e\u043c&#8230;<\/p>\n\n\n\n<p>\u0412 \u043a\u0430\u043a\u0438\u0435 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430 \u0438\u0434\u0443\u0442 \u043b\u044e\u0434\u0438, \u0437\u0430\u0441\u0442\u0438\u0433\u043d\u0443\u0442\u044b\u0435 \u0442\u0440\u0430\u0433\u0435\u0434\u0438\u0435\u0439? \u0412 \u0442\u0435, \u0447\u0442\u043e \u0431\u043b\u0438\u0436\u0435 \u043a \u0434\u043e\u043c\u0443. \u0418 \u0432 \u0442\u0435, \u0447\u0442\u043e \u043f\u043e\u0434\u0435\u0448\u0435\u0432\u043b\u0435. \u0412 \u043a\u0430\u043a\u0443\u044e \u0441\u0443\u043c\u043c\u0443 \u043e\u0431\u0445\u043e\u0434\u044f\u0442\u0441\u044f \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u044b?\n\u0412 \u0441\u0440\u0435\u0434\u043d\u0435\u043c 1200-1700 \u0440\u0443\u0431\u043b\u0435\u0439 \u043f\u043e \u0434\u0430\u043d\u043d\u044b\u043c \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u043d\u043e\u0439 \u0441\u043b\u0443\u0436\u0431\u044b \u00ab\u041d\u0430\u0442\u0440\u043e\u043d\u00bb. \u0412 \u0434\u0440\u0443\u0433\u0438\u0445 \u0431\u044e\u0440\u043e \u0441\u0443\u043c\u043c\u0443 \u043d\u0430\u0437\u044b\u0432\u0430\u044e\u0442, \u0441\u043e\u043e\u0442\u0432\u0435\u0442\u0441\u0442\u0432\u0435\u043d\u043d\u043e, \u043d\u0430\u043c\u043d\u043e\u0433\u043e \u043d\u0438\u0436\u0435, \u0447\u0442\u043e\u0431\u044b \u043a\u043b\u0438\u0435\u043d\u0442\u043e\u0432 \u043d\u0435 \u043e\u0442\u0442\u043e\u043b\u043a\u043d\u0443\u0442\u044c, \u043f\u0440\u0438\u0432\u043b\u0435\u0447\u044c. \u041d\u043e \u043a\u043e\u0433\u0434\u0430 \u0440\u0438\u00ad\u0442\u0443\u0430\u043b \u0437\u0430\u043a\u0430\u043d\u0447\u0438\u0432\u0430\u0435\u0442\u0441\u044f,\n\u0440\u043e\u0434\u0441\u0442\u0432\u0435\u043d\u043d\u0438\u043a\u0438 \u043d\u0430\u0447\u0438\u043d\u0430\u044e\u0442 \u043f\u043e\u0434\u00ad\u0441\u0447\u0438\u0442\u044b\u0432\u0430\u0442\u044c, \u0430 \u043f\u043e\u0434\u0441\u0447\u0438\u0442\u0430\u0432 &#8212; \u0435\u0449\u0435 \u0440\u0430\u0437 \u043f\u0440\u043e\u0441\u043b\u0435\u0437\u044f\u0442\u0441\u044f.<\/p>\n\n\n\n<p>&#8212; \u0427\u0435\u043b\u043e\u0432\u0435\u043a\u0443 \u0441 \u0443\u043c\u0435\u0440\u0448\u0438\u043c \u043d\u0435\u043a\u0443\u0434\u0430 \u0434\u0435\u0432\u0430\u0442\u044c\u0441\u044f &#8212; \u0440\u0430\u0441\u00ad\u0441\u043a\u0430\u0437\u044b\u0432\u0430\u0435\u0442 \u0421\u0432\u0435\u0442\u043b\u0430\u043d\u0430 \u041c\u0438\u0445\u0430\u0439\u043b\u043e\u0432\u043d\u0430, \u043d\u0435\u0434\u0430\u0432\u043d\u043e \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u0438\u0432\u0448\u0430\u044f \u043c\u0430\u0442\u044c. &#8212; \u041d\u0430 \u044d\u0442\u043e \u0438 \u0441\u0434\u0435\u043b\u0430\u043d \u0440\u0430\u0441\u0447\u0435\u0442. &#8212; \u0425\u043e\u0442\u0435\u043b\u0430 \u044f \u043f\u0430\n\u0441\u043e\u0441\u0435\u0434\u0441\u0442\u0432\u0443 \u043e\u0431\u0440\u0430\u0442\u0438\u0442\u044c\u0441\u044f \u0432 \u00ab\u041d\u0430\u0442\u0440\u043e\u043d\u00bb, \u0434\u0430 \u043e\u0442\u043a\u0443\u0434\u0430 \u043d\u0438 \u0432\u043e\u0437\u044c\u043c\u0438\u0441\u044c, \u043f\u043e\u044f\u0432\u0438\u043b\u0438\u0441\u044c\n\u0441\u043e\u0432\u0435\u0442\u0447\u0438\u043a\u0438-\u0434\u043e\u0431\u0440\u043e\u0445\u043e\u0442\u044b. \u00ab\u0414\u043e\u0440\u043e\u0433\u043e \u0442\u0430\u043c, \u0442\u0432\u0435\u0440\u0434\u044f\u0442, \u043b\u0443\u0447\u0448\u0435 \u0432 \u0434\u0440\u0443\u0433\u043e\u0435 \u0431\u044e\u0440\u043e \u0438\u0434\u0438, \u0442\u0430\u043c \u0443 \u043d\u0430\u0441\n\u0437\u043d\u0430\u043a\u043e\u043c\u044b\u0435 \u0440\u0430\u0431\u043e\u0442\u0430\u044e\u0442\u00bb. \u041f\u0440\u0438\u0448\u043b\u0438 \u043a \u00ab\u0437\u043d\u0430\u00ad\u043a\u043e\u043c\u044b\u043c\u00bb. \u0426\u0435\u043d\u0443 \u043d\u0430\u043c, \u043a\u043e\u043d\u0435\u0447\u043d\u043e, \u043d\u0430\u0437\u0432\u0430\u043b\u0438 \u043f\u0440\u0438\u0435\u043c\u043b\u0435\u00ad\u043c\u0443\u044e.\n\u0414\u043e\u0433\u043e\u0432\u043e\u0440 \u043f\u043e\u0434\u043f\u0438\u0441\u0430\u043b\u0438. \u0416\u0434\u0435\u043c&#8230;<\/p>\n\n\n\n<p>&#8212; \u041f\u0440\u0438\u0435\u0445\u0430\u043b\u0438 \u00ab\u043f\u043e\u0433\u0440\u0435\u0431\u0430\u043b\u044c\u0449\u0438\u043a\u0438\u00bb, &#8212; \u043f\u0440\u043e\u0434\u043e\u043b\u0436\u0430\u0435\u0442 \u043e\u043d\u0430, &#8212; \u0438 \u043d\u0430\u0447\u0430\u043b\u0438\u0441\u044c \u043d\u043e\u0432\u044b\u0435 \u0431\u0435\u0434\u044b. \u0423\u043c\u0435\u0440\u0448\u0430\u044f \u043b\u0435\u0436\u0438\u0442 \u043d\u0430 \u043f\u044f\u0442\u043e\u043c \u044d\u0442\u0430\u00ad\u0436\u0435, \u0430 \u0432 \u0434\u043e\u0433\u043e\u0432\u043e\u0440\u0435, \u043e\u043a\u0430\u0437\u0430\u043b\u043e\u0441\u044c, \u044d\u0442\u0430\u0436\u043d\u043e\u0441\u0442\u044c \u043d\u0435\u043e\u0431\u0433\u043e\u0432\u043e\u0440\u0435\u043d\u0430. \u0417\u0430 \u0432\u044b\u043d\u043e\u0441 \u0442\u0435\u043b\u0430 &#8212; \u0434\u043e\u0431\u0430\u0432\u044c. \u0420\u0430\u0439\u043e\u043d\n\u0434\u043e\u0441\u0442\u0430\u0432\u043a\u0438 \u0442\u043e\u0436\u0435 \u0431\u044b\u043b \u043d\u0435 \u0443\u0447\u0442\u0435\u043d.\n\u041f\u0440\u0438\u0431\u0430\u0432\u044c. \u0421\u0442\u043e\u0438\u043c\u043e\u0441\u0442\u044c \u043c\u043e\u0433\u0438\u043b\u044b \u0442\u0430\u043a\u0436\u0435 \u043e\u043a\u0430\u0437\u0430\u043b\u0430\u0441\u044c \u043d\u0435 \u00ab\u0432\u043a\u043b\u044e\u0447\u0435\u043d\u043d\u043e\u0439\u00bb.\n\u00ab\u041d\u0435 \u043d\u0430\u0448\u0430 \u0435\u043f\u0430\u0440\u0445\u0438\u044f\u00bb, -\u0441\u043a\u0430\u0437\u0430\u043b\u0438.\n\u0417\u043d\u0430\u0447\u0438\u0442, \u0435\u0449\u0435 \u0432\u044b\u043b\u043e\u0436\u0438 \u0434\u0435\u043d\u044c\u0433\u0438. \u041d\u0430 \u043a\u043b\u0430\u0434\u0431\u0438\u0449\u0435 \u043f\u0440\u0438\u0448\u043b\u043e\u0441\u044c \u043f\u0440\u0438\u043f\u043b\u0430\u00ad\u0442\u0438\u0442\u044c \u0438 \u0437\u0435\u043c\u043b\u0435\u043a\u043e\u043f\u0430\u043c, \u0438 \u0437\u0430\n\u0441\u0440\u043e\u0447\u043d\u043e\u0441\u0442\u044c, \u0438 \u0437\u0430 \u0430\u0432\u0442\u043e\u0431\u0443\u0441, \u0447\u0442\u043e\u0431\u044b \u043e\u0431\u0440\u0430\u0442\u043d\u043e \u043e\u0442\u0432\u0435\u0437 \u043f\u0440\u043e\u0449\u0430\u0432\u0448\u0438\u0445\u0441\u044f. \u041e\u0431\u0440\u0430\u0442\u043d\u044b\u0439 \u0440\u0435\u0439\u0441 \u0442\u0430\u043a\u0436\u0435 \u00ab\u043d\u0435 \u0443\u0447\u043b\u0438\u00bb. \u0412\u043e\u0442 \u0438 \u0432\u044b\u0448\u0435\u043b \u043d\u0430\u0448 \u0440\u0438\u0442\u0443\u0430\u043b \u0432 \u00ab\u0441\u0430\u043c\u0443\u044e \u0434\u0435\u0448\u0435\u0432\u0443\u044e \u0441\u0443\u043c\u00ad\u043c\u0443\u00bb, \u0432\u043a\u043e\u043f\u0435\u0435\u0447\u043a\u0443,\n\u0432 \u0434\u0432\u0435 \u0441 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u043e\u0439 \u0442\u044b\u0441\u044f\u0447\u0438.<\/p>\n\n\n\n<p>&#8212; \u0412 \u043d\u0430\u0440\u043e\u0434\u0435 \u0433\u043e\u0432\u043e\u0440\u044f\u0442: \u00ab\u0414\u043e\u0440\u043e\u0433\u043e, \u0434\u0430 \u043c\u0438\u043b\u043e; \u0434\u0435\u0448\u0435\u0432\u043e, \u0434\u0430 \u0433\u043d\u0438\u043b\u043e\u00bb. \u041c\u0443\u0434\u0440\u044b\u0439 \u043d\u0430\u0440\u043e\u0434 \u0443 \u043d\u0430\u0441. \u0422\u043e\u043b\u044c\u043a\u043e \u043c\u044b \u0447\u0430\u0441\u0442\u043e, \u043a \u0441\u043e\u0436\u0430\u043b\u0435\u043d\u0438\u044e, \u0437\u0430\u0431\u044b\u0432\u0430\u0435\u043c \u043d\u0430\u0440\u043e\u0434\u043d\u044b\u0435\n\u0437\u0430\u043f\u043e\u0432\u0435\u0434\u0438&#8230; \u0422\u044b \u0443\u0436, \u043a\u0430\u043a \u0436\u0443\u0440\u043d\u0430\u043b\u0438\u0441\u0442, \u043f\u043e\u0434\u0441\u043e\u0431\u0438, \u0441\u043a\u0430\u0436\u0438 \u043b\u044e\u0434\u044f\u043c \u043f\u0440\u0430\u0432\u0434\u0443; \u0447\u0442\u043e, \u0433\u0434\u0435,\n\u043f\u043e\u0447\u0435\u043c? \u0427\u0442\u043e\u0431\u044b \u0434\u0440\u0443\u0433\u0438\u0435 \u043d\u0435 \u043e\u0448\u0438\u0431\u0430\u043b\u0438\u0441\u044c, \u043d\u0435 \u0441\u0442\u0440\u0430\u0434\u0430\u043b\u0438, \u043d\u0435 \u0440\u0430\u0441\u043a\u043e\u0448\u0435\u043b\u0438\u0432\u0430\u043b\u0438\u0441\u044c. \u041f\u0440\u043e\u043d\u0438\u043a\u043d\u0443\u043b\u0441\u044f\n\u044f \u0441\u043e\u0432\u0435\u0442\u043e\u043c \u0437\u043d\u0430\u043a\u043e\u043c\u043e\u0439 \u0438 \u043e\u0431\u043e\u0448\u0435\u043b \u0440\u044f\u0434 \u043f\u043e\u0445\u043e\u0440\u043e\u043d\u043d\u044b\u0445 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432. \u0412 \u00ab\u041f\u0430\u0442\u0440\u043e\u043d\u0435\u00bb \u0434\u0430\u043b\u0438\n\u043e\u0431\u044a\u0435\u043a\u0442\u0438\u0432\u043d\u0443\u044e, \u0447\u0435\u0441\u0442\u043d\u0443\u044e \u0438\u043d\u0444\u043e\u0440\u043c\u0430\u0446\u0438\u044e, \u0440\u0430\u0437\u044a\u044f\u0441\u043d\u0438\u043b\u0438 \u043a\u0430\u0436\u00ad\u0434\u044b\u0439 \u043f\u0443\u043d\u043a\u0442 \u043f\u0440\u0435\u0439\u0441\u043a\u0443\u0440\u0430\u043d\u0442\u0430. \u0414\u043e\u0440\u043e\u0433\u043e?\n\u041d\u0435\u0442, \u0440\u0435\u0430\u043b\u044c\u043d\u043e: \u0446\u0435\u043d\u0430 \u043f\u043e\u0441\u043b\u0435 \u0432\u0441\u0435\u0445 \u0440\u0430\u0437\u044a\u044f\u0441\u043d\u0435\u043d\u0438\u0439 \u043e\u043a\u0430\u0437\u0430\u043b\u0430\u0441\u044c \u0432\u043f\u043e\u043b\u043d\u0435 \u043f\u0440\u0438\u0435\u043c\u043b\u0435\u043c\u043e\u0439. \u0410 \u043e\n\u043a\u0430\u0447\u0435\u0441\u0442\u0432\u0435 \u0443\u0441\u043b\u0443\u0433 \u0441\u0432\u0438\u0434\u0435\u0442\u0435\u043b\u044c\u0441\u0442\u0432\u043e\u0432\u0430\u043b\u0438 \u043c\u043d\u043e\u0433\u043e\u043b\u0435\u0442\u043d\u0438\u0439 \u043e\u043f\u044b\u0442 \u0438 \u0430\u0432\u0442\u043e\u0440\u0438\u0442\u0435\u0442 \u0444\u0438\u0440\u043c\u044b. \u0427\u0442\u043e \u0436\u0435\n\u043a\u0430\u0441\u0430\u0435\u0442\u0441\u044f \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u0445&#8230;<\/p>\n\n\n\n<p>\u0412 \u043e\u0441\u0442\u0430\u043b\u044c\u043d\u044b\u0445 \u0440\u0438\u0442\u0443\u0430\u043b\u044c\u043d\u044b\u0445 \u0430\u0433\u0435\u043d\u0442\u0441\u0442\u0432\u0430\u0445\n\u043c\u043d\u0435, \u043e\u0442\u00ad\u043a\u0440\u043e\u0432\u0435\u043d\u043d\u043e \u0433\u043e\u0432\u043e\u0440\u044f, \u043d\u0435 \u043f\u043e\u043d\u0440\u0430\u0432\u0438\u043b\u043e\u0441\u044c: \u0446\u0435\u043d\u044b \u043d\u0430\u0437\u044b\u0432\u0430\u00ad\u043b\u0438\u0441\u044c \u0441\u0430\u043c\u044b\u0435 \u00ab\u043d\u0438\u0437\u043a\u0438\u0435\u00bb, \u0441\u0430\u043c\u044b\u0435 \u00ab\u0441\u043c\u0435\u0448\u043d\u044b\u0435\u00bb, \u0441\u0430\u043c\u044b\u0435 \u0440\u0430\u0437\u043d\u044b\u0435 \u0438&#8230; \u043e\u0442\u0432\u043e\u0434\u0438\u043b\u0438 \u0432\u0437\u0433\u043b\u044f\u0434. \u041d\u043e \u043e\u0431\u0449\u0430\u044f \u0441\u0443\u043c\u043c\u0430 \u043d\u0438\u00ad\u043a\u0430\u043a\n\u043d\u0435 \u0432\u044b\u0440\u0438\u0441\u043e\u0432\u044b\u0432\u0430\u043b\u0430\u0441\u044c. \u0422\u043e \u0442\u0430\u043c, \u0442\u043e \u0437\u0434\u0435\u0441\u044c \u043d\u0435\u0438\u0437\u043c\u0435\u043d\u00ad\u043d\u043e \u043f\u043e\u043b\u0443\u0447\u0430\u043b\u043e\u0441\u044c, \u0447\u0442\u043e \u043f\u0440\u0438\u043f\u043b\u0430\u0447\u0438\u0432\u0430\u0442\u044c {\u0438 \u043d\u0435\u043c\u0430\u043b\u043e), \u0430 \u043f\u0440\u0438\u0434\u0435\u0442\u0441\u044f. \u041f\u043b\u0430\u0442\u0438\u0442\u044c, \u0435\u0441\u0442\u0435\u0441\u0442\u0432\u0435\u043d\u043d\u043e, \u043f\u0440\u0438\u0445\u043e\u0434\u0438\u0442\u0441\u044f. \u041d\u043e \u043f\u043e\u00ad\u0433\u0440\u0435\u0431\u0430\u043b\u044c\u043d\u044b\u0439\n\u0441\u0435\u0440\u0432\u0438\u0441 \u0434\u043e\u043b\u0436\u0435\u043d \u0431\u044b\u0442\u044c \u043a\u0430\u043a\u0438\u043c-\u0442\u043e \u0435\u0434\u0438\u043d\u044b\u043c: \u0441\u043f\u043e\u043a\u043e\u0439\u043d\u044b\u043c, \u0442\u0440\u0430\u0443\u0440\u043d\u044b\u043c, \u043c\u0438\u043b\u043e\u0441\u0435\u0440\u0434\u043d\u044b\u043c&#8230; \u0410 \u043d\u0435 \u0437\u0430\u0433\u0440\u0435\u00ad\u0431\u0443\u0449\u0438\u043c. \u041d\u0435 \u0442\u0440\u044e\u043a\u0430\u0447\u0435\u0441\u043a\u0438\u043c &#8212;